<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hzqing.seata.hzqpay.mapper.PayMapper">

    <insert id="insert" parameterType="pay">
        insert into db_pay
            (amount, create_time,order_id)
        values
            (#{amount},#{createTime},#{orderId})
    </insert>

</mapper>